{{#if (has-multiple-versions domain)}}
<select class="select version-select js-version">
  <option disabled>Select version
  {{#each (page-versions site.domains domain.name versions)}}
  <option value="{{{./url}}}" {{#if (eq ./string ../domain.version.string)}}selected{{/if}}>{{./string}}
  {{/each}}
</select>
{{/if}}
